<!-- AWARD NAME ATTRIBUTES -->
<!-- Attributes for the <award-name> element -->
<grammar xmlns="http://relaxng.org/ns/structure/1.0" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">
  <define name="award-name-atts">
    <ref name="jats-common-atts"/>
    <optional>
      <attribute name="xml:lang">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
    <ref name="might-link-atts"/>
  </define>
  <!-- AWARD DESCRIPTION ATTRIBUTES -->
  <!-- Attributes for the <award-desc> element -->
  <define name="award-desc-atts">
    <ref name="jats-common-atts"/>
    <optional>
      <attribute name="xml:lang">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
    <ref name="might-link-atts"/>
  </define>
  <!-- CONTRIBUTED RESOURCE GROUP ATTRIBUTES -->
  <!--
    Attributes for the 
    <contributed-resource-group> element
  -->
  <!-- FUNDING GROUP ATTRIBUTES -->
  <!--
    Attributes for the <funding-group>
    element
  -->
  <define name="funding-group-atts">
    <ref name="jats-common-atts"/>
    <optional>
      <attribute name="specific-use"/>
    </optional>
    <optional>
      <attribute name="xml:lang">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
  </define>
  <!-- FUNDING SOURCE ATTRIBUTES -->
  <!--                     Attributes for the <funding-source> element -->
  <define name="funding-source-atts">
    <ref name="jats-common-atts"/>
    <optional>
      <attribute name="rid">
        <data type="IDREFS"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="source-type"/>
    </optional>
    <optional>
      <attribute name="country"/>
    </optional>
    <optional>
      <attribute name="specific-use"/>
    </optional>
    <optional>
      <attribute name="xml:lang">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
    <ref name="might-link-atts"/>
  </define>
  <!-- FUNDING STATEMENT ATTRIBUTES -->
  <!--
    Attributes for the <funding-statement>
    element
  -->
  <define name="funding-statement-atts">
    <ref name="jats-common-atts"/>
    <optional>
      <attribute name="rid">
        <data type="IDREFS"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="specific-use"/>
    </optional>
    <optional>
      <attribute name="xml:lang">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
