<!-- ============================================================= -->
<!-- PARAMETER ENTITIES FOR CLASS OVER-RIDES -->
<!-- ============================================================= -->
<!-- ============================================================= -->
<!-- OVER-RIDE FORMATTING CLASS (format.ent) -->
<!-- ============================================================= -->
<!-- EMPHASIS RENDITION ELEMENTS -->
<!--
  All the face markup plus <ruby>            
  <!ENTITY % emphasis.class
                          "bold | fixed-case | italic | monospace | 
                           overline | roman |  sans-serif | sc | 
                           strike | underline |
                           ruby"                                       >
-->
<!-- FACE MARKUP CLASS ELEMENTS -->
<!--
  All the face markup without <ruby>         
  <!ENTITY % face-markup.class
                          "bold | fixed-case |italic | monospace | 
                           overline | roman | sans-serif | sc | strike |
                           underline"                                  >
-->
<!-- ============================================================= -->
<!-- OVER-RIDE INLINE CLASSES -->
<!-- ============================================================= -->
<!-- CITATION CLASS ELEMENTS -->
<!--
  Reference to an external document, as used
                          within, for example, the text of a
                          paragraph                                 
  <!ENTITY % citation.class
                          "citation-alternatives | element-citation | 
                           mixed-citation | nlm-citation"              >
-->
<!-- CITATION MINUS ALTERNATIVES CLASS ELEMENTS -->
<!--
  All the citation elements except the
  <citation-alternatives> element.
-->
<grammar xmlns="http://relaxng.org/ns/structure/1.0">
  <define name="citation-minus-alt.class">
    <choice>
      <ref name="element-citation"/>
      <ref name="mixed-citation"/>
      <ref name="nlm-citation"/>
    </choice>
  </define>
  <!-- CONTRIBUTOR INFORMATION -->
  <!--
                        Metadata about a contributor               
    <!ENTITY % contrib-info.class
                            "address | aff | aff-alternatives |
                             author-comment | bio | email | ext-link |
                             on-behalf-of | role | uri | xref"           >
  -->
  <!-- ============================================================= -->
  <!-- OVER-RIDE PARAGRAPH-LIKE ELEMENTS -->
  <!-- ============================================================= -->
  <!-- REST OF PARAGRAPH CLASS (MINUS PARAGRAPH) -->
  <!--
                        Information for the reader that is at the
                            same structural level as a Paragraph.
                            Contains all paragraph-level objects that are
                            not also used inside tables and excepting
                            also the paragraph element itself.         
    <!ENTITY % rest-of-para.class
                            "disp-quote | speech | statement |
                             verse-group"                                >
  -->
  <!-- ================== End Publishing Classes Customization ===== -->
  <!-- ================== New Publishing Classes Overrides for Minimal Customization ===== -->
  <!-- PHRASE CLASS ELEMENTS: Delete abbrev, index-term, index-term-range-end, milestone-end/-start -->
  <define name="phrase.class">
    <choice>
      <ref name="named-content"/>
      <ref name="styled-content"/>
    </choice>
  </define>
  <!--                     CONTRIBUTOR INFORMATION CLASS: Delete aff-alternatives, author-comment, on-behalf-of -->
  <define name="contrib-info.class">
    <choice>
      <ref name="address"/>
      <ref name="aff"/>
      <ref name="bio"/>
      <ref name="email"/>
      <ref name="etal"/>
      <ref name="ext-link"/>
      <ref name="role"/>
      <ref name="uri"/>
      <ref name="xref"/>
    </choice>
  </define>
  <define name="person-group-info.class">
    <choice>
      <ref name="aff"/>
      <ref name="etal"/>
      <ref name="role"/>
    </choice>
  </define>
  <!--                     AFFILIATIONS ALTERNATIVES CLASS: Delete aff-alternatives -->
  <define name="aff-alternatives.class">
    <ref name="aff"/>
  </define>
  <!--
    BIBLIOGRAPHIC REFERENCE (CITATION) CLASS: Delete annotation, collab-annotations, 
    gov, issue-part, part-title, 
    std, string-date, volume-id, size, name-alternatives
  -->
  <define name="references.class">
    <choice>
      <ref name="article-title"/>
      <ref name="chapter-title"/>
      <ref name="collab"/>
      <ref name="comment"/>
      <ref name="conf-acronym"/>
      <ref name="conf-sponsor"/>
      <ref name="conf-date"/>
      <ref name="conf-loc"/>
      <ref name="conf-name"/>
      <ref name="data-title"/>
      <ref name="date"/>
      <ref name="date-in-citation"/>
      <ref name="day"/>
      <ref name="edition"/>
      <ref name="email"/>
      <ref name="elocation-id"/>
      <ref name="etal"/>
      <ref name="ext-link"/>
      <ref name="fpage"/>
      <ref name="institution"/>
      <ref name="institution-wrap"/>
      <ref name="isbn"/>
      <ref name="issn"/>
      <ref name="issn-l"/>
      <ref name="issue"/>
      <ref name="issue-id"/>
      <ref name="issue-title"/>
      <ref name="lpage"/>
      <ref name="month"/>
      <ref name="name"/>
      <ref name="object-id"/>
      <ref name="page-range"/>
      <ref name="patent"/>
      <ref name="person-group"/>
      <ref name="pub-id"/>
      <ref name="publisher-loc"/>
      <ref name="publisher-name"/>
      <ref name="role"/>
      <ref name="season"/>
      <ref name="series"/>
      <ref name="source"/>
      <ref name="string-name"/>
      <ref name="supplement"/>
      <ref name="trans-source"/>
      <ref name="trans-title"/>
      <ref name="uri"/>
      <ref name="version"/>
      <ref name="volume"/>
      <ref name="volume-series"/>
      <ref name="year"/>
    </choice>
  </define>
  <!--                     NAMES CLASS: Delete anonymous, collab-alternatives, name-alternatives -->
  <define name="name.class">
    <choice>
      <ref name="collab"/>
      <ref name="name"/>
      <ref name="string-name"/>
    </choice>
  </define>
  <!--                     DISPLAY CLASS ELEMENTS: Delete answer, answer-set,  question-wrap, question-wrap-group, alternatives -->
  <define name="block-display.class">
    <choice>
      <ref name="address"/>
      <ref name="array"/>
      <ref name="block-alternatives"/>
      <ref name="boxed-text"/>
      <ref name="chem-struct-wrap"/>
      <ref name="code"/>
      <ref name="fig"/>
      <ref name="fig-group"/>
      <ref name="graphic"/>
      <ref name="media"/>
      <ref name="preformat"/>
      <ref name="supplementary-material"/>
      <ref name="table-wrap"/>
      <ref name="table-wrap-group"/>
    </choice>
  </define>
  <!-- DISPLAY CLASS ELEMENTS MINUS EXPLANATION -->
  <define name="block-display-minus-explanation.class">
    <choice>
      <ref name="address"/>
      <ref name="array"/>
      <ref name="boxed-text"/>
      <ref name="chem-struct-wrap"/>
      <ref name="code"/>
      <ref name="fig"/>
      <ref name="fig-group"/>
      <ref name="graphic"/>
      <ref name="media"/>
      <ref name="preformat"/>
      <ref name="supplementary-material"/>
      <ref name="table-wrap"/>
      <ref name="table-wrap-group"/>
    </choice>
  </define>
  <!-- FLOATING DISPLAY CLASS ELEMENTS: Delete alternatives -->
  <define name="floats-display.class">
    <choice>
      <ref name="block-alternatives"/>
      <ref name="boxed-text"/>
      <ref name="chem-struct-wrap"/>
      <ref name="code"/>
      <ref name="fig"/>
      <ref name="fig-group"/>
      <ref name="graphic"/>
      <ref name="media"/>
      <ref name="preformat"/>
      <ref name="supplementary-material"/>
      <ref name="table-wrap"/>
      <ref name="table-wrap-group"/>
    </choice>
  </define>
  <!-- CITATION CLASS ELEMENTS: Delete citation-alternatives -->
  <define name="citation.class">
    <choice>
      <ref name="element-citation"/>
      <ref name="mixed-citation"/>
    </choice>
  </define>
  <!-- ALTERNATIVES DISPLAY CLASS ELEMENTS: Delete nline-supplement-material, private-char -->
  <define name="alternatives-display.class">
    <choice>
      <ref name="array"/>
      <ref name="chem-struct"/>
      <ref name="code"/>
      <ref name="graphic"/>
      <ref name="inline-graphic"/>
      <ref name="inline-media"/>
      <ref name="media"/>
      <ref name="preformat"/>
      <ref name="supplementary-material"/>
      <ref name="table"/>
      <ref name="textual-form"/>
    </choice>
  </define>
  <!-- KEYWORD CLASS ELEMENTS: Delete compound-kwd, nested-kwd -->
  <define name="kwd.class">
    <ref name="kwd"/>
  </define>
  <!-- NESTED KEYWORD CLASS ELEMENTS: Delete compound-kwd -->
  <define name="nested-kwd.class">
    <ref name="kwd"/>
  </define>
  <!-- CONFERENCE CLASS: string-conf -->
  <define name="conference.class">
    <choice>
      <ref name="conf-acronym"/>
      <ref name="conf-date"/>
      <ref name="conf-name"/>
      <ref name="conf-num"/>
      <ref name="conf-loc"/>
      <ref name="conf-sponsor"/>
      <ref name="conf-theme"/>
    </choice>
  </define>
  <!-- DATE PARTS CLASS ELEMENTS: Delete era -->
  <define name="date-parts.class">
    <choice>
      <ref name="day"/>
      <ref name="month"/>
      <ref name="season"/>
      <ref name="year"/>
    </choice>
  </define>
  <!-- ADDRESS CLASS ELEMENTS: Delete fax -->
  <define name="address.class">
    <choice>
      <ref name="addr-line"/>
      <ref name="city"/>
      <ref name="country"/>
      <ref name="institution"/>
      <ref name="institution-wrap"/>
      <ref name="phone"/>
      <ref name="postal-code"/>
      <ref name="state"/>
    </choice>
  </define>
  <!--                     ADDRESS LINE CLASS ELEMENTS: Delete fax -->
  <define name="address-line.class">
    <choice>
      <ref name="city"/>
      <ref name="country"/>
      <ref name="institution"/>
      <ref name="institution-wrap"/>
      <ref name="phone"/>
      <ref name="postal-code"/>
      <ref name="state"/>
    </choice>
  </define>
  <!--                     EMPHASIS/RENDITION ELEMENTS: Delete fixed-case, overline, ruby, sans-serif, strike -->
  <define name="emphasis.class">
    <choice>
      <ref name="bold"/>
      <ref name="italic"/>
      <ref name="monospace"/>
      <ref name="roman"/>
      <ref name="sc"/>
      <ref name="underline"/>
    </choice>
  </define>
  <!-- FACE MARKUP ELEMENTS: Delete fixed-case, overline, sans-serif, strike -->
  <define name="face-markup.class">
    <choice>
      <ref name="bold"/>
      <ref name="italic"/>
      <ref name="monospace"/>
      <ref name="roman"/>
      <ref name="sc"/>
      <ref name="underline"/>
    </choice>
  </define>
  <!--                     INLINE DISPLAY CLASS ELEMENTS: private-char, alternatives -->
  <define name="inline-display.class">
    <choice>
      <ref name="inline-graphic"/>
      <ref name="inline-media"/>
